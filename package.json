{
    "name": "example-app-openapi-fetch",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
